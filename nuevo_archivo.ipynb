    "# nuevo archivo de pruebas \n",
    "\n",
    "def calcular_media(lista):\n",
    "    if not lista:\n",
    "        return 0\n",
    "    return sum(lista) / len(lista)\n",
    "\n",
    "# Ejemplo de uso\n",
    "lista = [1, 2, 3, 4, 5]\n",
    "media = calcular_media(lista)\n",
    "print(f\"La media de la lista es: {media}\")"
   ]
